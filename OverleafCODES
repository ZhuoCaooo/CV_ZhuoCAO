\resheading{Research Experience}
\begin{itemize}
\item
	\ressubheading{Traffic Flow Theory in Mixed Traffic Network}{UNSW, Sydney}{Supervisor: Dr.Wei Liu}{June. 2021 - June. 2022}
	\begin{itemize}
		\resitem{Aimed to evaluate the impact of the lane management policies on the mixed traffic model}
		\resitem{Extend traditional traffic assignment problem to mixed traffic with multiple equilibrium behavior assignment problems}
		\resitem{Introduce the platooning intensity parameter to lane management policies and link capacity performance analyzing}
		\resitem{System Optimization of the multi-class traffic assignment model under different CAV market penetration rates and lane management policies}
	\end{itemize}

\item
	\ressubheading{Neural Network Based Car Following Model Calibration}{UNSW, Sydney}{Supervisor: Dr.Changliu Liu}{May. 2021 - August. 2021}
	\begin{itemize}
		\resitem{Intended to develop a car-following model (CFM) based on the trajectory data from NGSIM datasets.}
		\resitem{Compared the performance of model-based CFM (IDM) and model-free CFM (Neural Network Model and Interaction Model) by pre-training on NGSIM data set and conducting online testing with a subset of trajectory data from NGSIM.}
		\resitem{Publication:\emph{On-demand Trajectory Prediction Based on Adaptable Interaction Car Following Model with Decreasing Tolerance}, 2021}
	\end{itemize}
	
\end{itemize}

